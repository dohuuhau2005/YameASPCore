<style type="text/css">
    .old-price,
    .new-price,
    discount {
        display: inline-block;
        margin-right: 10px;
        /* Tạo khoảng cách giữa hai giá */
    }



    .color-options {
        width: 55px;
        /* Đặt kích thước cho hình ảnh */
        height: 50px;
        border-radius: 50%;
        /* Tạo hình tròn cho hình ảnh nếu cần */
        object-fit: cover;
        /* Đảm bảo hình ảnh không bị méo */
        padding-left: 10px;
        gap: 10px;
        /* Tạo khoảng cách giữa các hình ảnh */
    }

    .old-price {
        text-decoration: line-through;
        color: black;
        font-size: 14px;
    }

    .new-price {
        color: red;
        font-size: 16px;
        font-weight: bold;
    }

    .w-100 {
        display: none;
    }

    * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
    }

    body {
        font-family: Arial, sans-serif;
        background-color: #f4f4f4;
    }

    .product-container {
        width: 90%;
        margin: 20px auto;
    }

    h2 {
        font-size: 24px;
        margin-bottom: 20px;
        text-align: left;
    }


    .product-grid01 {
        display: grid;
        grid-template-columns: repeat(2, 1fr);
        gap: 20px;

    }

    .product-item01 {
        background-color: white;
        padding: 10px;
        border-radius: 8px;
        text-align: center;
        box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
        max-width: 550px;
    }

    .product-grid {
        display: grid;
        grid-template-columns: repeat(4, 1fr);
        gap: 20px;

    }

    .product-item {
        background-color: white;
        padding: 10px;
        border-radius: 8px;
        text-align: center;
        box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
    }

    .product-item img {
        max-width: 100%;
        height: auto;
        border-radius: 8px;
    }

    .product-info {
        margin-top: 10px;

    }

    .old-price {
        text-decoration: line-through;
        color: black;
        font-size: 14px;
    }

    .new-price {
        color: red;
        font-size: 16px;
        font-weight: bold;
        margin-left: 5px;
    }

    .discount {
        background-color: none;
        color: black;
        padding: 3px;
        border-radius: 4px;
        font-size: 14px;
    }

    .color-options {
        margin-top: 10px;
        display: flex;
        /* Sử dụng flexbox để xếp hình ảnh theo hàng ngang */
        gap: 5px;
        /* Tạo khoảng cách giữa các hình ảnh */
    }

    .color-options01 {
        margin-top: 10px;
        display: flex;
        /* Sử dụng flexbox để xếp hình ảnh theo hàng ngang */
        gap: 20px;
        /* Tạo khoảng cách giữa các hình ảnh */
    }

    .color-options01 img {
        border-radius: 8px;
    }

    .product-item01 img {
        border-radius: 8px;
        max-width: 520px;
        height: auto
    }


    .color {
        display: inline-block;
        width: 15px;
        height: 15px;
        margin-right: 5px;
        border-radius: 50%;
        border: 1px solid #ccc;
    }

    .search-banner {
        display: flex;
        flex-direction: column;
        align-items: center;
        margin-top: 20px;
    }

    /* Định dạng cho thanh tìm kiếm */
    .search-bar select {
        width: 100%;
        max-width: 1200px;
        /* Chiều rộng của thanh chọn lọc */
        padding: 10px;
        font-size: 14px;
        border: 1px solid #ccc;
        border-radius: 4px;
        display: block;
        /* Để margin: auto hoạt động */
        margin: 0 auto;
        /* Căn giữa */
    }

    /* Định dạng cho banner hình ảnh */
    .banner {
        width: 100%;
        text-align: center;
    }

    .banner-image {
        width: 100%;
        max-width: 1200px;
        /* Giới hạn kích thước tối đa của ảnh */
        height: auto;
        border-radius: 10px;
    }

    .image-container {
        position: relative;
        width: 100%;
        height: auto;
        overflow: hidden;
    }

    .image {
        width: 100%;
        height: auto;
        transition: transform 0.5s ease;
    }

    .hover-image {
        position: absolute;
        top: 0;
        left: 100%;
        /* Đặt vị trí bên phải để hình ảnh ẩn */
        max-width: 100%;
        height: auto;
        transition: transform 0.5s ease;
        /* Hiệu ứng chuyển tiếp khi hover */
    }

    .image-container:hover .image {
        transform: translateX(-130%);
        /* Di chuyển hình ảnh chính sang trái */
        transition: transform 0.5s ease;
    }

    .image-container:hover .hover-image {
        transform: translateX(-100%);
        /* Di chuyển hình ảnh hover vào vị trí */
    }
</style>

